&lt;&#x2F;span&gt;&lt;span style=&quot;color:#f9f9f9;&quot;&gt;sudo&lt;&#x2F;span&gt;&lt;span style=&quot;color:#ffee80;&quot;&gt; flashrom&lt;&#x2F;span&gt;&lt;span style=&quot;color:#f9f9f9;&quot;&gt; -p&lt;&#x2F;span&gt;&lt;span style=&quot;color:#ffee80;&quot;&gt; ch341a_spi&lt;&#x2F;span&gt;&lt;span style=&quot;color:#f9f9f9;&quot;&gt; -r&lt;&#x2F;span&gt;&lt;span style=&quot;color:#ffee80;&quot;&gt; read2.rom
&lt;&#x2F;span&gt;&lt;span style=&quot;color:#f9f9f9;&quot;&gt;sudo&lt;&#x2F;span&gt;&lt;span style=&quot;color:#ffee80;&quot;&gt; flashrom&lt;&#x2F;span&gt;&lt;span style=&quot;color:#f9f9f9;&quot;&gt; -p&lt;&#x2F;span&gt;&lt;span style=&quot;color:#ffee80;&quot;&gt; ch341a_spi&lt;&#x2F;span&gt;&lt;span style=&quot;color:#f9f9f9;&quot;&gt; -r&lt;&#x2F;span&gt;&lt;span style=&quot;color:#ffee80;&quot;&gt; read3.rom
&lt;&#x2F;span&gt;&lt;span style=&quot;color:#f9f9f9;&quot;&gt;#&lt;&#x2F;span&gt;&lt;span style=&quot;font-style:italic;color:#0088ff;&quot;&gt; И проверяем суммы CRC32 или md5sum
&lt;&#x2F;span&gt;&lt;span style=&quot;color:#f9f9f9;&quot;&gt;md5sum&lt;&#x2F;span&gt;&lt;span style=&quot;color:#ffee80;&quot;&gt; read1.rom read2.rom read3.rom
&lt;&#x2F;span&gt;&lt;&#x2F;code&gt;&lt;&#x2F;pre&gt;
&lt;p&gt;Скачанный из remont-aud.net дамп биоса&lt;&#x2F;p&gt;
&lt;pre data-lang=&quot;sh&quot; style=&quot;background-color:#2b303b;color:#ffffff;&quot; class=&quot;language-sh &quot;&gt;&lt;code class=&quot;language-sh&quot; data-lang=&quot;sh&quot;&gt;&lt;span style=&quot;color:#f9f9f9;&quot;&gt;cksum&lt;&#x2F;span&gt;&lt;span style=&quot;color:#ffee80;&quot;&gt; asrock&lt;&#x2F;span&gt;&lt;span style=&quot;color:#ff628c;&quot;&gt;\ &lt;&#x2F;span&gt;&lt;span style=&quot;color:#ffee80;&quot;&gt;g41m-s.BIN
